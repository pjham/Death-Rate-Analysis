cppFunction ('LogicalVector pm(NumericalVector vec)
{
int n = vec.size();
LogicalVector ans(n);
for(int i=0;i<n;i++)
{
if(vec(i)>0)
{
ans(i)=TRUE;
}
else
{
ans(i)=FALSE;
}
}
return ans;
}')
