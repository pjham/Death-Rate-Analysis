cppFunction ('NumericVector colsumC(NumericMatrix mat)
{
int m=mat.nrow();
int n=mat.ncol();
NumericVector sum(n);
for(int i=0;i<n;i++)
{
sum(i) = 0;
for (int j=0;j<m;j++)
{
sum(i) = sum(i) + mat(j,i);
}
}
return(sum);
}')
mat <- matrix(runif(1e5),nrow = 1e3)
benchmark(colSums(mat),colsumC(mat),replications = 25)
mat <- matrix(runif(1e6),nrow = 1e3)
benchmark(colSums(mat),colsumC(mat),replications = 25)
mat <- matrix(runif(1e7),nrow = 1e3)
benchmark(colSums(mat),colsumC(mat),replications = 25)
